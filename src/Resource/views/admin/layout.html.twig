                                <i class="fa fa-instagram"></i>
                                <div>4524 +</div>
                                <p>Likes</p>
                            </div>
                        </div>
                    </div>#}
              {% endblock %}
          </div>
      </div>
      {% block javascripts %}
          <script src="{{ asset(domainPath~'/js/jquery.min.js') }}"></script>
          <script src="{{ asset(domainPath~'/js/popper/popper.min.js') }}"></script>
          <script src="{{ asset(domainPath~'/js/tether.min.js') }}"></script>
          <script src="{{ asset(domainPath~'/js/bootstrap.min.js') }}"></script>
          <script src="{{ asset(domainPath~'/js/jquery.cookie.js') }}"></script>
          <script src="{{ asset(domainPath~'/js/jquery.validate.min.js') }}"></script>
          {#<script src="{{ asset(domainPath~'/js/chart.min.js') }}"></script>#}
          <script src="{{ asset(domainPath~'/js/front.js') }}"></script>

